<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="90"
                android:gradientRadius="360"
                android:centerColor="@color/orange_FFA928"
                android:endColor="@color/orange_link_FFDE9C"
                android:startColor="@color/orange_FFA928"
                />
            <corners android:radius="1000dp" />
        </shape>
    </item>
    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
            <corners android:radius="1000dp" />
        </shape>
    </item>

</layer-list>